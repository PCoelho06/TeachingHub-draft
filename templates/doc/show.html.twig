{% extends 'base.html.twig' %}
{% block title %}
	{{ document.title }}
{% endblock
%}
{% block body %}
	<div class="container my-5">
		<h1 class="page-title">{{ document.title }}</h1>
		<div class="mx-auto w-75">
			<embed src="{{ asset('uploads/documents/' ~ document.filename) }}" width="100%" height="1000" type="application/pdf"/>
		</div>
		<a href="{{ asset('uploads/documents/' ~ document.filename) }}">Télécharger le document</a>
	</div>
{% endblock %}
